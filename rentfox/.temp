/* =upload box
----------------------------------------------- */
.uploadPlaceHolder {
	height: 105px;
	width: 114px;
	border: 1px solid #ccc;
	background: url("../img/upload_photo_bg.png") no-repeat center center;
	margin: 0 0 0 20px;
	cursor: pointer;
	overflow: hidden;
	}
.uploadPlaceHolder span {
	display: block;
	padding: 46px 0 46px 0;
	text-align: center;
	color: #0c3f84;
	text-decoration: underline;
	}
.uploadPlaceHolder.photo span {
	display: block;
	background: #333;
