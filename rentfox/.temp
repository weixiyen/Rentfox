#propertySetup .save label input#propertyZip {
	width: 87px;
	}
#propertySetup .save .action {
	clear: both;
	}
#propertySetup .save .action a.fixedLength {
	margin-right: 5px;
	width: 65px;
	}
#propertySetup .uploadPlaceHolder {
	float: right;
	}
#propertySetup .save .uploadPlaceHolder {
	margin-top: 17px;
	}


