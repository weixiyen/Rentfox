/* =Picture Viewer
----------------------------------------------- */
.picViewer .picDisplay {
	background: #000 url("../img/picviewer_frame_placeholder.png") no-repeat center center;
	width: 430px;
	height: 300px;
	}
.picViewer .picDisplay .frame {
	display: block;
	width: 430px;
	height: 300px;
	background: url("../img/picviewer_frame.png") no-repeat center center;
	}
.picViewer .picDisplay .frame img {
	display: none;
	}
.picViewer .picBar {
	background: url("../img/picviewer_bottom_bg.png") no-repeat;
	height: 33px;
	width: 430px;
	position: absolute;
	margin-top: -33px;
	}
.picViewer .picBar .picNav {
	margin: 7px 0 0 10px;
	}
.picViewer .picBar .picNav > div {
	float: left;
	}
.picViewer .picBar .picNav > div span{
	display: none;
	}
.picViewer .picBar .picNav .left{
	height: 17px;
	width: 18px;
	background: url("../img/picviewer_prev_arrow.png") no-repeat;
	cursor: pointer;
	}
.picViewer .picBar .picNav .right{
	height: 17px;
	width: 18px;
	background: url("../img/picviewer_next_arrow.png") no-repeat;
	cursor: pointer;
	}
.picViewer .picBar .picNav .left.active{
	background: url("../img/picviewer_prev_arrow_pressed.png") no-repeat;
	}
.picViewer .picBar .picNav .right.active{
	background: url("../img/picviewer_next_arrow_pressed.png") no-repeat;
	}
.picViewer .picBar .picNav .center{
	width: 70px;
	color: #fff;
	text-align: center;
	padding-top: 2px;
	}
.picViewer .picBar .picEditor {
	float: right;
	background: url("../img/ico/cog.png") no-repeat 100px center;
	width: 131px;
	height: 33px;
	cursor: pointer;
	}
.picViewer .picBar .picEditor .picOptions {
	padding-top: 5px;
	background: url("../img/picviewer_menu_top.png") no-repeat 0 top;
	margin-top: -55px;
	display: none;
	}
.picViewer .picBar .picEditor.on .picOptions {
	display: block;
	}
.picViewer .picBar .picEditor ul {
	background: url("../img/picviewer_menu_bg.png");
	height: 50px;
	}
.picViewer .picBar .picEditor ul li {
	color: #ccc;
	padding: 5px 5px 5px 30px;
	cursor: pointer;
	}
.picViewer .picBar .picEditor ul li.add {
	background: url("../img/ico/add_orange.png") no-repeat 10px 5px;
	}
.picViewer .picBar .picEditor ul li.delete {
	background: url("../img/ico/delete.png") no-repeat 10px 5px;
	}
.picViewer .picBar .picEditor ul li.delete:hover {
	background: #333 url("../img/ico/delete.png") no-repeat 10px 5px;
	}
.picViewer .droptext {
	position: absolute;
	display: none;
	color: #fff;
	font-size: 30px;
	text-align: center;
	margin: 100px 0 0 0;
	width: 430px;
	}
.picViewer .droptext.hover {
	display: block;
	}
.picViewer .droptext.on {
	display: block;
	color: yellow;
	}

