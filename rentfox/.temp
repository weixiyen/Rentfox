/* =unit table
----------------------------------------------- */
.unitTable {
	margin: 0 5px 0 5px;
	}
.unitTable table{
	margin-top: 25px;
	width: 100%;
	}
.unitTable thead {
	color: #333;
	background: #e3e3e3;
	position: fixed;
	display: block;
	margin: -43px 0 0 0;
	border-bottom: 1px solid #ccc;
	height: 42px;
	z-index: 200;
	}
.unitTable thead tr {
	}
.unitTable thead tr th {
	height: 42px;
	font-weight: bold;
	float: left;
	display: block;
	}
.unitTable thead tr th span {
	display: block;
	padding: 12px 0 0 10px;
	}
.unitTable tbody {
	background: #fff;
	display: block;
	}
.unitTable tbody tr {
	cursor: default;
	}
.unitTable tbody tr td {
	border-bottom: 1px solid #efefef;
	height: 30px;
	overflow: hidden;
	float: left;
	display: block;
	}
.unitTable tbody tr td span {
	display: block;
	padding: 8px 0 0 10px;
	}
.unitTable th.on {
	background: #c0cdd1;
	}
.unitTable td.on {
	background: #dbe6ea;
	}
.unitTable tr:hover td {
	background: #e6eadb;	
	}
.unitTable tr.selected td, .unitTable tr.selected:hover td {
	background: #333;
	color: #fff;
	}
.unitTable .pagination {
	float: right;
	font: normal 10px Verdana;
	color: #666;
	margin: 3px 10px 0 0;
	}
.unitTable .pagination.left {
	float: left;
	}
.unitTable .pagination > a {
	float: left;
	display: block;
	margin: 0 1px 0 1px;
	}
.unitTable .pagination > .control {
	height: 16px;
	width: 16px;
	background: no-repeat url("../img/ico/control_button_sprite.png");
	cursor: pointer;
	}
.unitTable .pagination > .control.disabled {
	cursor: default;
	}
.unitTable .pagination .pages {
	margin: 2px 5px 0 5px;	
	}
.unitTable .pagination .first {
	background-position: 0 -32px;
	}
.unitTable .pagination .first:active, .unitTable .pagination .first.disabled {
	background-position: -16px -32px;
	}
.unitTable .pagination .prev {
	background-position: 0 0;
	}
.unitTable .pagination .prev:active, .unitTable .pagination .prev.disabled {
	background-position: -16px 0;
	}
.unitTable .pagination .next {
	background-position: 0 -16px;
	}
.unitTable .pagination .next:active, .unitTable .pagination .next.disabled {
	background-position: -16px -16px;
	}
.unitTable .pagination .last {
	background-position: 0 -48px;
	}
.unitTable .pagination .last:active, .unitTable .pagination .last.disabled {
	background-position: -16px -48px;
	}
.unitTable th.sortable {
	cursor: pointer;
	}
.unitTable th.sortable:hover {
	background: #ccc;
	}
.unitTable th.text span {
	padding-top: 16px;
	}

/* =unit table
----------------------------------------------- */
.progressbox {
	padding: 10px;
	overflow: hidden;
	border: 1px solid #ccc;
	background: #f9f9f9;
	-webkit-border-radius: 3px;
	-moz-border-radius: 3px;
	border-radius: 3px;
	cursor: pointer;
	}
.progressbox:hover {
	background: #fff;
	}
.progressbox h3 {
	font-weight: bold;
	margin: 0 0 10px 0;
	}
.progressbox .bar {
	height: 8px;
	background: #333;
	-webkit-border-radius: 3px;
	-moz-border-radius: 3px;
	border-radius: 3px;
	}
.progressbox .bar span {
	height: 8px;
	background: #64ff45 url("../img/progressbar.png") no-repeat;
	display: block;
	width: 0;
	-webkit-border-radius: 3px;
	-moz-border-radius: 3px;
	border-radius: 3px;
	}
.progressbox label {
	padding-top: 5px;
	display: block;
	color: #666;
	cursor: pointer;
	}
