/* =layout
----------------------------------------------- */
#dashboard {
	overflow: auto;
	}

/* =search
----------------------------------------------- */
#dashboard .search {
	position: fixed;
	overflow: auto;
	width: 364px;
	min-height: 500px;
	top: 10%;
	text-align: center;
	background: url("../img/dash_bg.png") no-repeat center 85px;
	}
#dashboard .search .searchbox {
	padding: 180px 0 0 0;
	background: url("../img/logo_big.png") no-repeat center 50px;
	}
#dashboard .search .searchbox .box input {
	width: 236px;
	background: url("../img/searchbox_big.png") center top no-repeat;
	border: 0;
	margin: 0;
	padding: 7px 10px 15px 10px;
	font-size: 18px;
	}
#dashboard .search .searchbox .find .button {
	width: 150px;
	margin-left: -75px;
	left: 50%;
	position: relative;
	}
#dashboard .search .stats {
	padding-top: 20px;
	}
#dashboard .search .stats p {
	margin-bottom: 10px;
	}

	
/* =stats
----------------------------------------------- */
#dashboard .progressbox {
	margin: 0 55px 20px 55px;
	}


/* =properties
----------------------------------------------- */
#dashboard .props {
	float: right;
	width: 300px;
	margin-top: 40px;
	}
#dashboard .props .prop {
	overflow: auto;
	clear: both;
	margin: 0 0 20px 30px;
	padding: 5px;
	cursor: pointer;
	display: block;
	text-decoration: none;
	color: #333;
	}
#dashboard .props .prop:hover dl {
	color: #0c3f84;
	}
#dashboard .props .prop:hover .thumb .frame {
	background: url("../img/thumb_frame_blue.png") no-repeat center center;
	}
#dashboard .props .prop .pic {
	float: left;
	}
#dashboard .props .prop dl {
	float: left;
	margin-left: 10px;
	padding: 2px;
	}
#dashboard .props .prop dl dt {
	font-weight: bold;
	display: block;
	margin-bottom: 5px;
	}
#dashboard .props .prop dl dd {
	display: block;
	float: left;
	}
#dashboard .props .prop dl dd.tenants {
	padding: 0 10px 0 10px;
	margin: 0 10px 0 10px;
	border-left: 1px solid #ccc;
	border-right: 1px solid #ccc;
	}
#dashboard .props .prop dl dd .title {
	display: block;
	margin-bottom: 5px
	}
#dashboard .props .prop dl dd .data {
	display: block;
	}
#dashboard .props .empty {
	margin: -15px 0 0 30px;
	}
#dashboard .props .empty .button.mini {
	margin-bottom: 15px;
	}
#dashboard .props .empty .nothing {
	clear: both;
	background: #ddd;
	padding: 10px;
	border-right: 1px solid #ccc;
	border-bottom: 1px solid #ccc;
	-webkit-border-radius: 3px;
	-moz-border-radius: 3px;
	border-radius: 3px;
	overflow: hidden;
	}

/* =pulse
----------------------------------------------- */
#dashboard .pulse {
	float: left;
	width: 300px;
	margin-top: 20px;
	padding-bottom: 60px;
	}


/* =background
----------------------------------------------- */
body#homepage {
    background: #fafafa center top no-repeat url('../img/home_bg.png');
    }
#homepage .all {
    width: 984px;
    margin: auto;
    }
#homepage a {
    cursor: pointer;
    }
#homepage strong {
    font-weight: bold;
    }
    
/* =sign in box
----------------------------------------------- */
#homepage .login {
    float: right;
    margin: 76px 10px 8px 0;
    }
#homepage .login div, #homepage .login a {
    float: left;
    margin: 0 3px 0 0;
    }
#homepage .login a {
    width: 87px;
    height: 23px;
    display: block;
    background: no-repeat url('../img/login_button_bg.png');
    text-align: center;
    color: #fff;
    }
#homepage .login a.signup {
    background-position: 0 -23px;
    }
#homepage .login a:active, #homepage .login a.active {
    background-position: 0 -46px;
    }
#homepage .login span {
    display: block;
    padding: 4px 0 0 0;
    }
#homepage .login input {
    border: 0;
    padding: 0;
    margin: 4px 0 0 0;
    background: none;
    color: #666;
    }
#homepage .login input.active {
    font: bold 12px Verdana;
    color: #000;
    }
#homepage .login .username input {
    width: 140px;    
    }
#homepage .login .password input {
    width: 127px;
    }

/* =main panel - tour & slides
----------------------------------------------- */
#homepage .promoMain {
    clear: both;
    overflow: hidden;
    }
#homepage .promoMain .tour {
    float: left;
    padding: 200px 0 0 30px;
    }
#homepage .promoMain .tour a.tourButton {
    height: 263px;
    width: 408px;
    background: url('../img/home_tour.png') no-repeat;
    display: block;
    }
#homepage .promoMain .tour a.tourButton:active {
    background-position: 0 -277px;
    }
#homepage .promoMain .slides {
    float: right;
    }
#homepage .promoMain .slideViewer {
    height: 489px;
    width: 487px;
    background: no-repeat url('http://static.rentfox.net/slide_1.png');
    cursor: pointer;
    }
#homepage .promoMain .slideNav {
    margin: 465px 0 0 210px;
    position: absolute;
    }
#homepage .promoMain .slideNav li{
    display: block;
    float: left;
    height: 8px;
    width: 8px;
    margin-right: 5px;
    background: url('../img/slide_nav.png') no-repeat;
    cursor: pointer;
    }
#homepage .promoMain .slideNav li:hover{
    background-position: 0 -8px;
    }
#homepage .promoMain .slideNav li.on{
    background-position: 0 -16px;
    }
    
/* =promos
----------------------------------------------- */
#homepage .promos {
    overflow: hidden;
    padding: 0 0 34px 0;
    }
#homepage .promos .promo {
    float: left;
    width: 246px;
    }
#homepage .promos .promo.new {
    background: no-repeat url('../img/promo_label_new.png') right top;
    }
#homepage .promos .promo .wrap {
    padding: 25px 15px 0 15px;    
    }
#homepage .promos .promo .comingSoon {
    float: right;
    display: block;
    width: 78px;
    height: 16px;
    background: no-repeat url('../img/coming_soon_bg.png');
    color: #fff;
    font: bold 11px "Myriad Pro", Arial;
    text-align: center;
    line-height: 1.6em;
    }
#homepage .promos .promo h3 {
    font: bold 16px "Myriad Pro", Arial;
    }
#homepage .promos .promo h5 {
    font: 16px "Myriad Pro", Arial;
    color: #666;
    padding: 10px 0 13px 0;
    }
#homepage .promos .promo p {
    font: 13px "Myriad Pro", Arial;
    height: 100px;
    line-height: 1.3em;
    }

/* =about and pricing
----------------------------------------------- */
