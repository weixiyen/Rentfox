	width: 125px;
	display: none;
	padding-bottom: 5px;
	}
.filterList .options ul {
	background: url("../img/filter_body.png") no-repeat center top;
	padding: 10px 0 10px 0
	}
.filterList .options ul li {
	cursor: pointer;
	display: block;
	padding: 3px 23px 3px 0;
	}
.filterList .options ul li:hover {
	background: #333;
	}
.filterList .options ul li.on {
	background: url("../img/check_mark.png") 106px center no-repeat;
	font-weight: bold;
	}
.filterList:hover label {
